  "private": true,
  "license": "MIT",
  "main": "e2etests.js",
  "dependencies": {
    "amqp10": "^2.2.2",
    "azure-iot-common": "1.0.1",
    "azure-iot-device": "1.0.1",
    "azure-iot-device-amqp": "1.0.1",
    "azure-iot-device-http": "1.0.1",
    "azure-iothub": "1.0.1",
    "bluebird": "^3.3.0",
