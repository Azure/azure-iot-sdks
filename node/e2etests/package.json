    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "chai": "^3.3.0",
    "jshint": "^2.8.0",
    "mocha": "^2.3.3"
  },
  "scripts": {
    "lint": "jshint --show-non-errors .",
    "unittest-min": "echo \"No tests\"",
